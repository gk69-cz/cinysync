{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/dist/public/assets/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/dist/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/index.js"
    }
  ]
}